{% extends 'base.html.twig' %}

{% block title %}Adress{% endblock %}

{% block body %}
    <h1>Adress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adress.id }}</td>
            </tr>
            <tr>
                <th>FirstLine</th>
                <td>{{ adress.firstLine }}</td>
            </tr>
            <tr>
                <th>SecondLine</th>
                <td>{{ adress.secondLine }}</td>
            </tr>
            <tr>
                <th>PostalCode</th>
                <td>{{ adress.PostalCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ adress.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ adress.country }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ adress.latitude }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ adress.longitude }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_adress_index') }}">back to list</a>

    <a href="{{ path('app_adress_edit', {'id': adress.id}) }}">edit</a>

    {{ include('adress/_delete_form.html.twig') }}
{% endblock %}
